type Pojazd {
    id: ID!
    vin: String!
    numerRejestracyjny: String
    wlasciciel: Klient
}

type Klient {
    id: ID!
	imie: String!
	nazwisko: String!
    email: String!
    haslo: String!
    pojazdy: [Pojazd!] @relation
}

